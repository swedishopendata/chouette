<?xml version="1.0" encoding="UTF-8"?>
<datasources xmlns="http://www.jboss.org/ironjacamar/schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.jboss.org/ironjacamar/schema
        http://docs.jboss.org/ironjacamar/schema/datasources_1_0.xsd">

	<datasource jndi-name="java:jboss/datasources/stip" pool-name="stip" enabled="true" use-java-context="true">
		<connection-url>jdbc:h2:mem:stip-inmemory;INIT=CREATE SCHEMA IF NOT EXISTS dbo;</connection-url>
		<driver-class>org.h2.Driver</driver-class>
		<driver>h2</driver>
		<security>
			<user-name>sa</user-name>
			<password></password>
		</security>
	</datasource>

</datasources>
